{% extends 'boxed-layout.html.twig' %}

{% block title %}Forecast{% endblock %}

{% block content %}
    <h1>Forecast</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ forecast.id }}</td>
            </tr>
            <tr>
                <th>TempaeratureCelsius</th>
                <td>{{ forecast.tempaeratureCelsius }}</td>
            </tr>
            <tr>
                <th>Humidity</th>
                <td>{{ forecast.humidity }}</td>
            </tr>
            <tr>
                <th>WindSpeed</th>
                <td>{{ forecast.windSpeed }}</td>
            </tr>
            <tr>
                <th>Pressure</th>
                <td>{{ forecast.pressure }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_forecast_index') }}">back to list</a>

    <a href="{{ path('app_forecast_edit', {'id': forecast.id}) }}">edit</a>

    {{ include('forecast/_delete_form.html.twig') }}
{% endblock %}
